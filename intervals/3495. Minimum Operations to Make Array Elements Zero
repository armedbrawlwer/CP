typedef long long ll;
class Solution {
public:
    long long minOperations(vector<vector<int>>& arr) {
        int n = arr.size();
        long long ans=0;
        for(auto nums:arr){
            ll st = nums[0],end = nums[1];
            ll ops=0;
            for(ll d=1,prev=1;d<17;d++){
                ll cur = prev*4;
                ll lt = max(st,prev);
                ll rt = min(end,cur-1);
                if(rt>=lt){
                    ops += (rt-lt+1)*d;
                }
                prev = cur;

            }
            ans += (ops+1)/2;
        }
        return ans;
    }
};
