class Solution {
public:
    int sortPermutation(vector<int>& nums) {
        //have to take and operation of all misplaced elements
        int ans =-1;
        int n = nums.size();
        for(int i=0;i<n;i++){
            if(i!=nums[i]){
                if(ans==-1)ans = nums[i];
                else ans &= nums[i];
            }
        }
        return ans==-1?0:ans;
    }
};
